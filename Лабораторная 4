#include <iostream>;
int main()
{
	int a = 6, b = 4, s, p, r = 100, c = 6, n = 8, sum, raz, m, ch;
	float pi = 3.14, d, sr;
	s = a * b; //вычисление площади
	p = 2 * a + 2 * b; //вычисление пириметраж
	d = r * pi; //вычисление радиуса
	sr = (a + b) / 2; //вычисление среднее арифмитическое двух чисел
	sum = n + c; //вычисление суммы двух ненуливых числа
	raz = n - c; //вычисление разности двух ненуливых числа
	m = n * c; //вычисление произвдения двух ненуливых числа
	ch = n * n + c * c; //вычисление частного двух ненуливых числа
	printf("result % d\ns: %d\np: %d\nd: %f\nsr: %f\nsum: %d\nraz: %d\nm: %d\nch: %d", s, s, p, d, sr, sum, raz, m, ch); //вывел значения периметра, площади, диаметра, сред. арифмитического, суммы, разности, произведения, частного квадратов
	return 0;
}

