#include <iostream>
using namespace std;
int main()
{
    setlocale(LC_ALL, "rus");
    float x;
    cout << "\n  Задание 5\n\n Введите ещё одно значение х для нахождения значения другой функции ";
    scanf_s("%d", &x);

    float y = 4 * (x - 3) * (x - 3) * (x - 3) * (x - 3) * (x - 3) * (x - 3) - 7 * (x - 3) * (x - 3) * (x - 3) + 2;

    printf("\n\n Ответ \n\n Задание 5 \n\n y = %f \n", y);

    return 0;
}
