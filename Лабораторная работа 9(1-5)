#include <iostream>;
using namespace std;
int main() {
	setlocale(LC_ALL, "rus");
	int n, sec, k, m, z, d, c2, a, b, l, s, y, ce;
	cout << "\n\n Задание 1 \n\n Введите количество секунд ";
	scanf_s("%d", &n);
	cout << "\n\n Задание 2-3 \n\n Введите день этого года(от 1 до 365) ";
	scanf_s("%d", &k);
	cout << " Введите каким номером недели было 1 января(от 1 до 7) ";
	scanf_s("%d", &m);
	cout << " \n\n Задание 4 \n\n Введите целое число А ";
	scanf_s("%d", &a);
	cout << " Введите целое число В ";
	scanf_s("%d", &b);
	cout << " Введите целое число С ";
	scanf_s("%d", &c2);
	cout << "\n\n Задание 5 \n\n Введите год(любое целое положительное число) ";
	scanf_s("%d", &y);

	sec = n % 60;

    d = (k + 3) % 7;

    z = ((k + m - 2) % 7) + 1;


	l = (a / c2) * (b / c2);
	s = a * b - l * c2*c2;
	
	ce = ((y - 1) / 100) + 1;

	printf("\n\n Ответы \n\n Задание 1 \n\n Количество секунд с начала последней минуты %d \n\n Задание 2 \n\n День недели %d\n\n Задание 3 \n\n День недели %d\n\n Задание 4 \n\n Количество квадратов, размещенных на прямоугольнике %d \n\n Площадь незанятой части прямоугольника %d \n\n Задание 5 \n\n Столетие %d \n\n", sec, d, z, l, s, ce);
	return 0;;
}
