#include <iostream>;
using namespace std;
int main() {
	setlocale(LC_ALL, "rus");
	int a, b, z, k, c, s, v;
	cout << " Введите значение А = ";
	scanf_s("%d", &a);
	cout << " Введите значение В = ";
	scanf_s("%d", &b);
	cout << " Введите значение С = ";
	scanf_s("%d", &c);
	v = a;
	z = b;
	s = c;
	a = z;
	b = v;
	c = z;
	a = s;
	printf("\n\nОтветы\n\nА = %d\n\nB = %d\n\nC = %d", a, b,c);

}
